  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "rollup -c",
    "test": "jest --collectCoverage --",
    "format:check": "prettier --check \"**/*.{js,jsx,yml,yaml,json.css}\"",
