{
	"name": "codemash",
	"version": "1.2.36",
	"description": "CodeMash tools for Node and javascript developers. Access codemash.io tools from npm",
	"main": "dist/index.cjs.js",
	"module": "dist/index.esm.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "rollup -c",
		"test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts' --timeout 3600000",
		"test-grep": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register --timeout 3600000 -g",
		"format:check": "prettier --check \"**/*.{js,jsx,yml,yaml,json.css}\"",
		"format": "prettier --write \"**/*.{js,jsx,yml,yaml,json.css}\"",
		"commit": "cz",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx --quiet",
		"lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
	},
	"files": [
		"dist/*",
		"src/*"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/codemash-io/CodeMash.Js.git"
	},
	"keywords": [
		"baas",
		"backend",
		"backend-as-a-service",
		"codemash",
		"database",
		"files",
		"push",
		"emails",
		"code",
		"serverless",
		"logs",
		"payments"
	],
	"author": "Domantas Jovaisas",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/codemash-io/CodeMash.Js/issues"
	},
	"homepage": "https://codemash.io",
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"@babel/preset-typescript": "^7.16.0",
		"@commitlint/cli": "^12.0.0",
		"@commitlint/config-conventional": "^12.0.0",
		"@rollup/plugin-babel": "^5.2.1",
		"@rollup/plugin-node-resolve": "^10.0.0",
		"@rollup/plugin-replace": "^2.3.4",
		"@types/chai": "^4.2.18",
		"@types/chai-as-promised": "^7.1.4",
		"@types/cookie": "^0.4.1",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.11.1",
		"@types/whatwg-fetch": "0.0.33",
		"@typescript-eslint/eslint-plugin": "^5.8.0",
		"@typescript-eslint/parser": "^5.8.0",
		"babel-jest": "^26.6.3",
		"chai": "^4.3.4",
		"chai-as-promised": "^7.1.1",
		"commitizen": "^4.2.3",
		"cz-conventional-changelog": "^3.3.0",
		"dotenv": "^8.2.0",
		"eslint": "^7.3.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-config-standard": "^14.1.1",
		"eslint-plugin-import": "^2.21.2",
		"eslint-plugin-jest": "^25.3.0",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"eslint-plugin-standard": "^4.0.1",
		"mocha": "^8.4.0",
		"prettier": "2.2.1",
		"rollup": "^2.33.1",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.31.1",
		"semantic-release": "^17.3.9",
		"ts-mocha": "^8.0.0",
		"ts-node": "9.0.0",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^4.3.5"
	},
	"dependencies": {
		"@servicestack/client": "^1.1.1",
		"cross-fetch": "^3.1.4",
		"eslint-config-airbnb-base": "^15.0.0",
		"husky": "^5.1.1",
		"node-fetch": "^2.6.1"
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
		}
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
