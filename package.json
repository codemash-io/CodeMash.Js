{
  "name": "codemash",
  "version": "1.2.36",
  "description": "CodeMash tools for Node and javascript developers. Access codemash.io tools from npm",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "ts-types": " tsc --emitDeclarationOnly --outDir dist",
    "build": "rimraf build && node ./esbuild.config.js && npm run ts-types",
    "test:coverage": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' nyc mocha -r ts-node/register 'tests/**/*.ts' --timeout 3600000",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts' --timeout 3600000",
    "format:check": "prettier --check \"**/*.{js,jsx,yml,yaml,json.css}\"",
    "format": "prettier --write \"**/*.{js,jsx,yml,yaml,json.css}\"",
    "commit": "cz",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codemash-io/CodeMash.Js.git"
  },
  "nyc": {
    "include": "src",
    "exclude": [
      "src/types"
    ]
  },
  "keywords": [
    "baas",
    "backend",
    "backend-as-a-service",
    "codemash",
    "database",
    "files",
    "push",
    "emails",
    "code",
    "serverless",
    "logs",
    "payments"
  ],
  "author": "Domantas Jovaisas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codemash-io/CodeMash.Js/issues"
  },
  "homepage": "https://codemash.io",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.16.0",
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "@faker-js/faker": "^6.0.0-alpha.3",
    "@types/chai": "^4.2.18",
    "@types/chai-as-promised": "^7.1.4",
    "@types/cookie": "^0.4.1",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.11.1",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "add": "^2.0.6",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^8.2.0",
    "esbuild": "^0.14.13",
    "esbuild-node-externals": "^1.4.1",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.3.9",
    "ts-mocha": "^8.0.0",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.5.5",
    "yarn": "^1.22.17"
  },
  "dependencies": {
    "@servicestack/client": "^1.1.1",
    "deep-parse-json": "^1.0.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
